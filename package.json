{
  "name": "bot.ext_taon",
  "version": "1.0.0",
  "description": "My awesome bot for whatsapp",
  "main": "./dist/index.js",
  "scripts": {
    "start": "electron-forge start",
    "start-elec": "npm run build && electron dist/index.js",
    "dev": "npx ts-node src/index.ts",
    "build": "tsc && copyfiles -f src/index.html dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeandroParisi/bot.ext_taon.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeandroParisi/bot.ext_taon/issues"
  },
  "homepage": "https://github.com/LeandroParisi/bot.ext_taon#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "cep-promise": "^4.1.2",
    "copyfiles": "^2.4.1",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
    "nedb-promises": "^5.0.2",
    "node-fetch": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2",
    "venom-bot": "^4.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/electron": "^1.6.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "electron": "^16.0.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "bot.ext_taon"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
