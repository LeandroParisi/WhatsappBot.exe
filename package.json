{
  "name": "whatsapp-bot",
  "version": "0.0.1",
  "description": "My awesome bot for whatsapp",
  "main": "./index.js",
  "scripts": {
    "start": "npm run pre-build:dev && electron build/index.js",
    "dev": "npx ts-node src/index.ts",
    "release": "npm run pre-build:release && electron-builder --win --publish always",
    "build": "npm run pre-build:release && electron-builder --win --publish never",
    "pack": "npm run pre-build:release && electron-builder build --win",
    "pre-build:release": "npm run clean-up && npm run compile",
    "pre-build:dev": "npm run compile",
    "compile": "rm -rf build && tsc && copyfiles -f src/index.html build && copyfiles -f package.json build && copyfiles -f yarn.lock build && cp -R ./src/Electron/WebPage build/WebPage",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-up": "rm -rf ./tokens && rm -rf databases"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeandroParisi/bot.ext_taon.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeandroParisi/bot.ext_taon/issues"
  },
  "homepage": "https://github.com/LeandroParisi/bot.ext_taon#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "cep-promise": "^4.1.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.6.5",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
    "nedb-promises": "^5.0.2",
    "node-fetch": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.3",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2",
    "venom-bot": "^4.0.5"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "electron": "^16.1.0",
    "electron-builder": "^22.14.13",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
